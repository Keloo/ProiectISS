<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="description" content="Proiect Colectiv Panda">
        <meta name="author" content="Panda">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

        <title>{% block title %}Proiect Colectiv{% endblock %}</title>
        {% block stylesheets %}
            {% stylesheets
                'bundles/app/css/bootstrap.min.css'
                'bundles/app/css/font-awesome.min.css'
                'bundles/app/css/smartadmin-production.min.css'
                'bundles/app/css/smartadmin-skins.min.css'
                'bundles/app/css/demo.min.css'
                'bundles/app/css/your_style.css'
                filter='cssrewrite' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
    </head>
    <body class="animated fadeInDown">
        {% include 'board/components/header.html.twig' %}
        {% include 'board/components/left_panel.html.twig' %}

        {% block body %}{% endblock %}

        {% include 'board/components/footer.html.twig' %}

        {% block javascripts %}
            {% javascripts
            '@AppBundle/Resources/public/js/libs/jquery-2.0.2.min.js'
            '@AppBundle/Resources/public/js/libs/jquery-ui-1.10.3.min.js'
            '@AppBundle/Resources/public/js/bootstrap/bootstrap.min.js'
            '@AppBundle/Resources/public/js/notification/SmartNotification.min.js'
            '@AppBundle/Resources/public/js/smartwidgets/jarvis.widget.min.js'
            '@AppBundle/Resources/public/js/app.config.js'
            '@AppBundle/Resources/public/js/app.min.js'
            '@AppBundle/Resources/public/js/plugin/*/*'
            %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
